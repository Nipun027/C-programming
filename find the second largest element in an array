#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<limits.h>
int main() {

    int n;
    scanf("%d", &n);
    int arr[100];
    for(int i=0; i<n; i++){
        scanf("%d", &arr[i]);
        
    }
   int largest = INT_MIN;
    int second_largest = INT_MIN;
    
    for(int i = 0; i < n; i++) {
        if(arr[i] > largest) {
            second_largest = largest;
            largest = arr[i];
        }
        else if(arr[i] > second_largest && arr[i] != largest) {
            second_largest = arr[i];
        }
    }
    
    if(second_largest == INT_MIN) {
        printf("No second largest element\n");
    } else {
        printf("%d\n", second_largest);
    }
    return 0;
}
